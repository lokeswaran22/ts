# PostgreSQL Database Configuration for Render.com
# Copy this file to .env and fill in your values

# Render.com PostgreSQL URL (get this from your Render PostgreSQL service)
DATABASE_URL=postgres://user:password@host:5432/dbname

# Set to 'production' for Render.com deployment
NODE_ENV=production

# Port (Render.com sets this automatically, default is 3000 for local)
PORT=3000
